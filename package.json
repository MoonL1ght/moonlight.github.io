{
  "name": "moonl1ght.github.io",
  "version": "0.0.4",
  "description": "moonl1ght's blog",
  "homepage": "https://moonl1ght.github.io",
  "scripts": {
    "build": "better-npm-run build:prod",
    "dev": "better-npm-run dev",
    "__build:prod": "webpack --config ./webpack/webpack.prod.config.js --progress",
    "clean": "rimraf *.js && rimraf *.css && rimraf *.html"
  },
  "betterScripts": {
    "build:prod": {
      "command": "npm run clean && npm run __build:prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "node ./server/main.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.4.1"
  },
  "author": "moonl1ght",
  "license": "Apache",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.2.1",
    "react": "^16.7.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "better-npm-run": "^0.1.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3",
    "base-href-webpack-plugin": "^2.0.0"
  }
}
